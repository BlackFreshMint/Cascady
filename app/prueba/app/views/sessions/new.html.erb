<center>
<h1>Sign in</h1>

<%= form_with url: sign_in_path do |form| %>


<%= form.label :email %><br>
<%= form.text_field :email , placeholder: "email@example.com"%><br>

<%= form.label :password %><br>
<%= form.password_field :password, placeholder: "password" %><br>

<%= form.submit "sign in" %>

<% end %>
</center>
